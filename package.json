{
  "name": "skeleton",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@smui/button": "^6.0.0-beta.4",
    "@smui/card": "^6.0.0-beta.4",
    "@smui/dialog": "^6.0.0-beta.4",
    "@smui/icon-button": "^6.0.0-beta.5",
    "@smui/layout-grid": "^6.0.0-beta.4",
    "@smui/textfield": "^6.0.0-beta.4",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "smui-theme": "^6.0.0-beta.4",
    "svelte": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "graphql": "^16.1.0",
    "subscriptions-transport-ws": "^0.11.0",
    "svelte-apollo": "^0.4.0"
  }
